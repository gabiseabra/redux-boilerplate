{
  "name": "redux-boilerplate",
  "version": "2.6.2",
  "description": "React + Redux + Webpack 3 boilerplate",
  "scripts": {
    "start": "node dist/server.js",
    "watch": "babel-node webpack/devServer.js --hot",
    "test": "mocha \"./{test,src}/**/*.spec.{js,jsx}\"",
    "lint": "eslint src test scripts",
    "prebuild": "cross-env NODE_ENV=production npm run build:vendor",
    "build": "webpack --config=webpack/config.babel.js",
    "build:dev": "cross-env NODE_ENV=development npm run build --",
    "build:prod": "cross-env NODE_ENV=production npm run build --",
    "build:client": "webpack --config=webpack/bundles/client.babel.js",
    "build:server": "webpack --config=webpack/bundles/server.babel.js",
    "build:vendor": "webpack --config=webpack/bundles/vendor.babel.js",
    "build:static": "cross-env NODE_ENV=production babel-node scripts/static.js > public/index.html",
    "clean": "npm run clean:client & npm run clean:server & npm run clean:static",
    "clean:client": "rimraf public/dist & rimraf public/appcache & rimraf public/sw.js",
    "clean:server": "rimraf dist",
    "clean:static": "rimraf public/index.html"
  },
  "author": "Gabriela Seabra",
  "license": "MIT",
  "homepage": "https://github.com/gabiseabra/redux-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabiseabra/redux-boilerplate.git"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-decorators": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.9",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "chai": "^4.1.2",
    "chai-enzyme": "^1.0.0-beta.0",
    "chai-things": "^0.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "dotenv": "^4.0.0",
    "enhanced-resolve": "^3.4.1",
    "enzyme": "^3.1.1",
    "enzyme-adapter-react-16": "^1.0.4",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fontello-webpack-plugin": "^0.4.6",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.3.0",
    "mocha": "^3.5.0",
    "node-mocks-http": "^1.6.6",
    "normalize-css": "^2.3.1",
    "offline-plugin": "^4.8.4",
    "postcss": "^6.0.12",
    "postcss-cssnext": "^3.0.2",
    "postcss-global-import": "^1.0.1",
