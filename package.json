{
  "name": "redux-boilerplate",
  "version": "2.0.0",
  "description": "React + Redux + Webpack 3 boilerplate",
  "scripts": {
    "start": "concurrently --kill-others \"npm run api\" \"npm run server\"",
    "watch": "concurrently --kill-others \"babel-node api/server.js\" \"babel-node webpack/devServer.js --hot\"",
    "lint": "eslint src",
    "prebuild": "cross-env NODE_ENV=production npm run build:vendor",
    "build": "webpack --config=webpack/config.babel.js --progress --colors --",
    "build:dev": "cross-env NODE_ENV=development npm run build --",
    "build:prod": "cross-env NODE_ENV=production npm run build --",
    "build:client": "webpack --config=webpack/bundles/client.babel.js --progress --colors",
    "build:server": "webpack --config=webpack/bundles/server.babel.js --progress --colors",
    "build:vendor": "webpack --config=webpack/bundles/vendor.babel.js --progress --colors",
    "build:static": "cross-env NODE_ENV=production babel-node scripts/static.js > public/index.html",
    "clean": "npm run clean:client & npm run clean:server & npm run clean:static",
    "clean:client": "rimraf public/dist & rimraf public/appcache & rimraf public/sw.js",
    "clean:server": "rimraf dist",
    "clean:static": "rimraf public/index.html",
    "server": "node dist/server.js",
    "api": "node dist/api.js"
  },
  "author": "Gabriela Seabra",
  "license": "MIT",
  "homepage": "https://github.com/gabiseabra/redux-boilerplate",
  "repository": {
    "type": "git",
    "url": "https://github.com/gabiseabra/redux-boilerplate.git"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-decorators": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "concurrently": "^3.4.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "dotenv": "^4.0.0",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "fontello-webpack-plugin": "^0.4.5",
    "normalize-css": "^2.3.1",
    "offline-plugin": "^4.8.3",
    "postcss": "^6.0.8",
    "postcss-cssnext": "^3.0.2",
    "postcss-global-import": "^1.0.1",
    "postcss-import-url": "^2.2.0",
    "postcss-loader": "^2.0.6",
    "postcss-partial-import": "^4.1.0",
    "postcss-url": "^7.1.1",
    "prop-types": "^15.5.10",
    "react-addons-perf": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "redux-logger": "^2.8.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "stylelint": "^8.0.0",
