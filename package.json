{
  "name": "redux-boilerplate",
  "version": "1.0.0",
  "description": "React + Redux + Webpack 2 boilerplate",
  "scripts": {
    "start": "concurrent --kill-others \"npm run api\" \"npm run server\"",
    "watch": "concurrent --kill-others \"babel-node api/server.js\" \"babel-node webpack/devServer.js\"",
    "prebuild": "better-npm-run build:vendor",
    "build": "better-npm-run build",
    "build:client": "better-npm-run build:client",
    "build:server": "better-npm-run build:server",
    "build:vendor": "better-npm-run build:vendor",
    "clean": "npm run clean:client && npm run clean:server",
    "clean:client": "rimraf ./public/dist",
    "clean:server": "rimraf ./dist",
    "server": "node dist/server.js",
    "api": "node dist/api.js"
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config=webpack/config.babel.js --progress --colors -p",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:client": "webpack --config=webpack/client.babel.js --progress --colors",
    "build:server": "webpack --config=webpack/server.babel.js --progress --colors",
    "build:vendor": {
      "command": "webpack --config=webpack/vendor.babel.js --progress --colors -p",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "author": "Gabriela Seabra",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-transform-decorators": "^6.22.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "better-npm-run": "0.0.14",
    "concurrently": "^3.4.0",
    "css-loader": "^0.26.2",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.0.0",
    "fontello-webpack-plugin": "^0.1.0",
    "normalize-css": "^2.3.1",
    "offline-plugin": "^4.6.1",
    "postcss": "^5.2.13",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.0",
    "redux-logger": "^2.8.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.2",
    "stylelint": "^7.8.0",
    "stylelint-config-css-modules": "^0.1.0",
    "stylelint-config-standard": "^16.0.0",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-merge": "^2.6.1",
    "webpack-node-externals": "^1.5.4"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "babel-polyfill": "^6.22.0",
    "compression": "^1.6.2",
    "express": "^4.14.1",
    "express-http-proxy": "^0.11.0",
    "http": "0.0.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-helmet": "^4.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.3",
    "serialize-javascript": "^1.3.0",
    "serve-favicon": "^2.4.0"
  }
}
